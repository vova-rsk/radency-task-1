{{#each this}}
      <tr class="table__row" data-id={{id}}>
        <th>
          <div class="table__col-thumb table__col-thumb--icon">
            <div class="table__icon" data-category="{{category}}"></div>
          </div>
        </th>
        <th>
          <div class="table__col-thumb table__col-thumb--note-category">
            {{category}}
          </div>
          </th>
        <th>
          <div class="table__col-thumb table__col-thumb--active" data-category="{{category}}" data-value>
            {{active}}
          </div>
        </th>
        <th>
          <div class="table__col-thumb table__col-thumb--last table__col-thumb--archived" data-category="{{category}}" data-value>
            {{archived}}
          </div>
        </th>
      </tr>
{{/each}}