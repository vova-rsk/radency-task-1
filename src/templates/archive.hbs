{{#each this}}
      <tr class="table__row" data-id={{id}}>
        <th>
          <div class="table__col-thumb table__col-thumb--icon">
            <div class="table__icon" data-category="{{category}}"></div>
          </div>
        </th>
        <th>
          <div class="table__col-thumb table__col-thumb--name">
            {{name}}
          </div>
        </th>
        <th>
          <div class="table__col-thumb table__col-thumb--created">
            {{created}}
          </div>
        </th>
        <th>
          <div class="table__col-thumb table__col-thumb--category">
            {{category}}
          </div>
        </th>
        <th>
          <div class="table__col-thumb table__col-thumb--content">
            {{content}}
          </div>
        </th>
        <th>
          <div class="table__col-thumb table__col-thumb--dates">
            {{#if dates}} {{dates.[0]}}, {{dates.[1]}} {{/if}}
          </div>
        </th>
        <th>
          <div class="table__col-thumb table__col-thumb--buttons note-ctrl-btns" data-id={{id}}>
            <button type="button" class="table__button" name="edit" data-id={{id}} data-icon="edit">
            </button>
            <button type="button" class="table__button" name="archive" data-id={{id}} data-icon="to-active">
            </button>
            <button type="button" class="table__button" name="remove" data-id={{id}} data-icon="remove">
            </button>
            <div class="checkbox-container visually-hidden">
              <label>
                <input type="checkbox" />
                <span></span>
              </label>              
            </div>
          </div>
        </th>
      </tr>
{{/each}}